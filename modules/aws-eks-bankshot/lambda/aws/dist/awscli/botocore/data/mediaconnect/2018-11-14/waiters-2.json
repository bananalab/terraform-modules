{
  "version": 2,
  "waiters": {
    "FlowActive": {
      "acceptors": [
        {
          "argument": "Flow.Status",
          "expected": "ACTIVE",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "Flow.Status",
          "expected": "STARTING",
          "matcher": "path",
          "state": "retry"
        },
        {
          "argument": "Flow.Status",
          "expected": "UPDATING",
          "matcher": "path",
          "state": "retry"
        },
        {
          "expected": 500,
          "matcher": "status",
          "state": "retry"
        },
        {
          "expected": 503,
          "matcher": "status",
          "state": "retry"
        },
        {
          "argument": "Flow.Status",
          "expected": "ERROR",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 3,
      "description": "Wait until a flow is active",
      "maxAttempts": 40,
      "operation": "DescribeFlow"
    },
    "FlowDeleted": {
      "acceptors": [
        {
          "expected": 404,
          "matcher": "status",
          "state": "success"
        },
        {
          "argument": "Flow.Status",
          "expected": "DELETING",
          "matcher": "path",
          "state": "retry"
        },
        {
          "expected": 500,
          "matcher": "status",
          "state": "retry"
        },
        {
          "expected": 503,
          "matcher": "status",
          "state": "retry"
        },
        {
          "argument": "Flow.Status",
          "expected": "ERROR",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 3,
      "description": "Wait until a flow is deleted",
      "maxAttempts": 40,
      "operation": "DescribeFlow"
    },
    "FlowStandby": {
      "acceptors": [
        {
          "argument": "Flow.Status",
          "expected": "STANDBY",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "Flow.Status",
          "expected": "STOPPING",
          "matcher": "path",
          "state": "retry"
        },
        {
          "expected": 500,
          "matcher": "status",
          "state": "retry"
        },
        {
          "expected": 503,
          "matcher": "status",
          "state": "retry"
        },
        {
          "argument": "Flow.Status",
          "expected": "ERROR",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 3,
      "description": "Wait until a flow is in standby mode",
      "maxAttempts": 40,
      "operation": "DescribeFlow"
    }
