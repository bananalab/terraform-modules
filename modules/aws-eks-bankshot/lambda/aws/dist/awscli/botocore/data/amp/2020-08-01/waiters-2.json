{
  "version": 2,
  "waiters": {
    "WorkspaceActive": {
      "acceptors": [
        {
          "argument": "workspace.status.statusCode",
          "expected": "ACTIVE",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "workspace.status.statusCode",
          "expected": "UPDATING",
          "matcher": "path",
          "state": "retry"
        },
        {
          "argument": "workspace.status.statusCode",
          "expected": "CREATING",
          "matcher": "path",
          "state": "retry"
        }
      ],
      "delay": 2,
      "description": "Wait until a workspace reaches ACTIVE status",
      "maxAttempts": 60,
      "operation": "DescribeWorkspace"
    },
    "WorkspaceDeleted": {
      "acceptors": [
        {
          "expected": "ResourceNotFoundException",
          "matcher": "error",
          "state": "success"
        },
        {
          "argument": "workspace.status.statusCode",
          "expected": "DELETING",
          "matcher": "path",
          "state": "retry"
        }
      ],
      "delay": 2,
      "description": "Wait until a workspace reaches DELETED status",
      "maxAttempts": 60,
      "operation": "DescribeWorkspace"
    }
  }
}
