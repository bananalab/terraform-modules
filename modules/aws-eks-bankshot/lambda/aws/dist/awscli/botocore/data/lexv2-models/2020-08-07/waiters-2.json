{
  "version": 2,
  "waiters": {
    "BotAliasAvailable": {
      "acceptors": [
        {
          "argument": "botAliasStatus",
          "expected": "Available",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "botAliasStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botAliasStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 10,
      "description": "Wait until a bot alias is available",
      "maxAttempts": 35,
      "operation": "DescribeBotAlias"
    },
    "BotAvailable": {
      "acceptors": [
        {
          "argument": "botStatus",
          "expected": "Available",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "botStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botStatus",
          "expected": "Inactive",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 10,
      "description": "Wait until a bot is available",
      "maxAttempts": 35,
      "operation": "DescribeBot"
    },
    "BotExportCompleted": {
      "acceptors": [
        {
          "argument": "exportStatus",
          "expected": "Completed",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "exportStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "exportStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 10,
      "description": "Wait until a bot has been exported",
      "maxAttempts": 35,
      "operation": "DescribeExport"
    },
    "BotImportCompleted": {
      "acceptors": [
        {
          "argument": "importStatus",
          "expected": "Completed",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "importStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "importStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 10,
      "description": "Wait until a bot has been imported",
      "maxAttempts": 35,
      "operation": "DescribeImport"
    },
    "BotLocaleBuilt": {
      "acceptors": [
        {
          "argument": "botLocaleStatus",
          "expected": "Built",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "NotBuilt",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 10,
      "description": "Wait until a bot locale is built",
      "maxAttempts": 35,
      "operation": "DescribeBotLocale"
    },
    "BotLocaleCreated": {
      "acceptors": [
        {
          "argument": "botLocaleStatus",
          "expected": "Built",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "ReadyExpressTesting",
          "matcher": "path",
          "state": "success"
