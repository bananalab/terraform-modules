        {
          "argument": "botLocaleStatus",
          "expected": "NotBuilt",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 10,
      "description": "Wait unit a bot locale is created",
      "maxAttempts": 35,
      "operation": "DescribeBotLocale"
    },
    "BotLocaleExpressTestingAvailable": {
      "acceptors": [
        {
          "argument": "botLocaleStatus",
          "expected": "Built",
          "matcher": "path",
          "state": "success"
