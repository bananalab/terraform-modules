        {
          "argument": "botLocaleStatus",
          "expected": "ReadyExpressTesting",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botLocaleStatus",
          "expected": "NotBuilt",
          "matcher": "path",
          "state": "failure"
        }
      ],
      "delay": 10,
      "description": "Wait until a bot locale build is ready for express testing",
      "maxAttempts": 35,
      "operation": "DescribeBotLocale"
    },
    "BotVersionAvailable": {
      "acceptors": [
        {
          "argument": "botStatus",
          "expected": "Available",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "botStatus",
          "expected": "Deleting",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "botStatus",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        },
        {
          "expected": 404,
          "matcher": "status",
          "state": "retry"
        }
      ],
      "delay": 10,
      "description": "Wait until a bot version is available",
      "maxAttempts": 35,
      "operation": "DescribeBotVersion"
    }
  }
}
