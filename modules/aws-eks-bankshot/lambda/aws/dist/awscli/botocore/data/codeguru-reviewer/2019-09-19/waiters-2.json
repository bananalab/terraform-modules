{
  "version": 2,
  "waiters": {
    "CodeReviewCompleted": {
      "acceptors": [
        {
          "argument": "CodeReview.State",
          "expected": "Completed",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "CodeReview.State",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "CodeReview.State",
          "expected": "Pending",
          "matcher": "path",
          "state": "retry"
        }
      ],
      "delay": 10,
      "description": "Wait until a code review is complete.",
      "maxAttempts": 180,
      "operation": "DescribeCodeReview"
    },
    "RepositoryAssociationSucceeded": {
      "acceptors": [
        {
          "argument": "RepositoryAssociation.State",
          "expected": "Associated",
          "matcher": "path",
          "state": "success"
        },
        {
          "argument": "RepositoryAssociation.State",
          "expected": "Failed",
          "matcher": "path",
          "state": "failure"
        },
        {
          "argument": "RepositoryAssociation.State",
          "expected": "Associating",
          "matcher": "path",
          "state": "retry"
        }
      ],
      "delay": 10,
      "description": "Wait until a repository association is complete.",
      "maxAttempts": 30,
      "operation": "DescribeRepositoryAssociation"
    }
  }
}
